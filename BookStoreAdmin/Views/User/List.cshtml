
@{
    Layout = null;
}
<h2> LIST USERS </h2>

<table>
    <thead>
        <tr> 
            <th>User Name</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>&nbsp;</th>
        </tr> 
    </thead>
    <tbody>
      <tr *ngFor="let user of (store.select('users') | async)?.users">
            <td></td>
            <td>{{user.first_name}}</td>
            <td>{{user.last_name}}</td>
            <td>&nbsp;</td>
        </tr>  
    </tbody>
</table>

{{(store.select('users') | async | json)}}